{
  "plugins": ["vue", "prettier"],
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/vue",
    "plugin:vue/recommended"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "$": "readonly"
  },
  "rules": {
    "prettier/prettier": "warn",
    "vue/max-attributes-per-line": "off",
    "vue/html-self-closing": "off",
    "vue/singleline-html-element-content-newline": "off",
    "vue/html-indent": "off"
  }
}