{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": "readonly"
  },
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/vue"
  ],
  "rules": {
    "prettier/prettier": "warn"
  }
}